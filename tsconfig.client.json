{
    "extends": "./tsconfig.json",
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "outDir": "./dist/client",
      "rootDir": "./src",
      "skipLibCheck": true,
      "strict": true,
      "noImplicitAny": false
    },
    "include": ["src/**/*.ts", "src/**/*.tsx"]
  }