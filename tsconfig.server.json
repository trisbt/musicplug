{
  "extends": "./tsconfig.json",
  "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "outDir": "./dist",
      "rootDir": "./server",
      "skipLibCheck": true,
      "strict": true,
      "noImplicitAny": false
  },
  "include": ["server/**/*.ts"]
}
